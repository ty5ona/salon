{
  "name": "salon-booking-system",
  "version": "10.28.0",
  "description": "WordPress plugin for salon booking and appointment scheduling",
  "scripts": {
    "compile:sass": "sass scss:css -w",
    "compile:sass:build": "sass scss:css --style=compressed",
    "serve": "browser-sync start --proxy 'localhost:8080' --files 'css/*.css, *.php, js/*.js'",
    "start": "npm-run-all --parallel compile:sass serve",
    "build": "npm-run-all compile:sass:build",
    "watch": "gulp watch",
    "gulp:build": "gulp",
    "dev": "npm-run-all --parallel compile:sass watch"
  },
  "dependencies": {
    "browser-sync": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.83.0"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "gulp": "^4.0.2",
    "gulp-less": "^5.0.0",
    "gulp-livereload": "^1.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-transifex": "^0.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "gulp-wp-pot": "^2.5.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
